{
  "$schema": "../node_modules/@tauri-apps/cli/schema.json",
  "build": {
    "beforeBuildCommand": "yarn generate",
    "beforeDevCommand": "yarn dev",
    "devPath": "http://127.0.0.1:3000/",
    "distDir": "../dist"
  },
  "package": {
    "productName": "Discord RPC Maker",
    "version": "3.0.0"
  },
  "tauri": {
    "allowlist": {
      "all": false,
      "clipboard": {
        "writeText": true
      }
    },
    "systemTray": {
      "iconAsTemplate": true,
      "iconPath": "icons/iconTemplate.png"
    },
    "bundle": {
      "active": true,
      "category": "Utility",
      "copyright": "",
      "appimage": {
      },
      "deb": {
        "depends": []
      },
      "externalBin": [],
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "identifier": "dev.t1c.drpcm",
      "longDescription": "Discord RPC Maker lets you make and manage custom Discord Rich Presences with buttons!",
      "macOS": {
        "entitlements": null,
        "exceptionDomain": "",
        "frameworks": [],
        "providerShortName": null,
        "signingIdentity": null
      },
      "resources": [],
      "shortDescription": "Cross-platform Discord Rich Presence Generator, WITH BUTTONS!",
      "targets": "all",
      "windows": {
        "certificateThumbprint": null,
        "digestAlgorithm": "sha256",
        "timestampUrl": ""
      }
    },
    "security": {
      "csp": null
    },
    "updater": {
      "active": false
    },
    "windows": [
      {
        "fullscreen": false,
        "height": 600,
        "resizable": true,
        "title": "Discord RPC Maker",
        "width": 800
      }
    ]
  }
}
